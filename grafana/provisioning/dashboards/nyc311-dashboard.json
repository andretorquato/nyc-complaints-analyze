            "type": "postgres",
            "uid": "PostgreSQL"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH ranked_complaints AS (\n  SELECT\n    l.borough,\n    ct.complaint_type,\n    COUNT(*) AS count,\n    ROUND(\n      (COUNT(*)::numeric / SUM(COUNT(*)) OVER (PARTITION BY l.borough)) * 100,\n      2\n    ) AS proporcao,\n    RANK() OVER (PARTITION BY l.borough ORDER BY COUNT(*) DESC) AS rank\n  FROM complaints c\n  JOIN locations l\n    ON c.location_id = l.location_id\n  JOIN complaint_types ct\n    ON c.complaint_type_id = ct.complaint_type_id\n  WHERE (\n    '${borough_single}' = ''\n    OR l.borough = '${borough_single}'\n  )\n  GROUP BY\n    l.borough,\n    ct.complaint_type\n)\nSELECT\n  borough,\n  complaint_type,\n  count,\n  proporcao,\n  rank\nFROM ranked_complaints\nWHERE rank <= ${topN:raw}\nORDER BY rank;\n",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
            "groupBy": [
              {
                "property": {
                  "type": "string"
                },
                "type": "groupBy"
              }
            ],
            "limit": 50
          }
        }
      ],
